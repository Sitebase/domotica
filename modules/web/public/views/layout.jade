doctype html
html
	head
		link(rel="stylesheet", href="/css/flatty.css")
		script(src="//ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js")
		script(src="/js/bootstrap.min.js")
	body(data-twttr-rendered='true')
		header.navbar.navbar-static-top.bs-docs-nav(id='top', role='banner')
			.navbar.navbar-default.navbar-fixed-top
				.container
					.navbar-header
						button.navbar-toggle.collapsed(data-toggle='collapse', type='button', data-target='.bs-navbar-collapse')
							span.sr-only Toggle navigation
							span.icon-bar
							span.icon-bar
							span.icon-bar
						a.navbar-brand(href='../') Home automation
					nav.collapse.navbar-collapse.bs-navbar-collapse
						ul.nav.navbar-nav
							each item in items
								li
									a #{item.label}
		.container
			.col-md-9
				h1 Hey #{title}
			.col-md-3
				h3 TEster
